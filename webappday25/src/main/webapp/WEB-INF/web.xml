<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>webappday25</display-name>
  
 
  <servlet-mapping>
    <servlet-name>Welcome</servlet-name>
    <url-pattern>/begin</url-pattern>
  </servlet-mapping> 
  <servlet>
    <servlet-name>Welcome</servlet-name>
    <servlet-class>ai.jobiak.webapp.Welcome</servlet-class>
  </servlet>
  
  <servlet>
  		<servlet-name>Factorial</servlet-name>
  		<servlet-class>ai.jobiak.webapp.FactorialServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  		<servlet-name>Factorial</servlet-name>
  		<url-pattern>/fact</url-pattern>
  </servlet-mapping>
  
   <servlet>
  		<servlet-name>Jdbc</servlet-name>
  		<servlet-class>ai.jobiak.webapp.JdbcServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  		<servlet-name>Jdbc</servlet-name>
  		<url-pattern>/jdbc</url-pattern>
  </servlet-mapping>
  
   <servlet>
  		<servlet-name>LifeCycle</servlet-name>
  		<servlet-class>ai.jobiak.webapp.LifeCycleServlet</servlet-class>
  		
  </servlet>
  
  <servlet-mapping>
  		<servlet-name>LifeCycle</servlet-name>
  		<url-pattern>/go</url-pattern>
  </servlet-mapping>
  
  <servlet>
  		<servlet-name>Signup</servlet-name>
  		<servlet-class>ai.jobiak.http.SignupServlet</servlet-class>
  		
  </servlet>
  
  <servlet-mapping>
  		<servlet-name>Signup</servlet-name>
  		<url-pattern>/signup.html</url-pattern>
  </servlet-mapping>
  
<welcome-file-list>
<welcome-file>links.html</welcome-file>
</welcome-file-list>
  

</web-app>